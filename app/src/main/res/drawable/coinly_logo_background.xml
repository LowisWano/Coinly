<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Yellow background circle -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#FFF0D07A" />
            <size android:width="120dp" android:height="120dp" />
        </shape>
    </item>
    
    <!-- Blue shape -->
    <item android:top="10dp" android:left="10dp" android:right="10dp" android:bottom="10dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF4A7ABF" />
            <corners android:radius="40dp" />
            <rotate android:fromDegrees="45" />
        </shape>
    </item>
    
    <!-- Gold coin -->
    <item android:top="40dp" android:left="40dp" android:right="0dp" android:bottom="0dp">
        <shape android:shape="oval">
            <solid android:color="#FFD4AF37" />
            <size android:width="50dp" android:height="50dp" />
        </shape>
    </item>
    
    <!-- Dark blue accent -->
    <item android:top="60dp" android:left="15dp" android:right="20dp" android:bottom="0dp">
        <shape android:shape="oval">
            <solid android:color="#FF1A4B8C" />
            <size android:width="40dp" android:height="40dp" />
        </shape>
    </item>
</layer-list>